.faq_center {
	margin-top: 30px;
	margin-bottom: 30px;
	width: 1230px;
	min-height: 600px;
}
.faq_center_title{
	font-size: 16px;
	color: #fff;
	line-height: 40px;
	padding-left: 24px;
	background: #1eb7c5;
}

#faq_left{
	width: 200px;
	vertical-align: top;
	float: left;
	background-color: #fff;
}
#container .new_faq_center {
	margin-left: 210px;
	padding-top: 20px;
	width: 1020px;
	background:none;
	box-shadow: none;
	min-height: 443px;
	background-color: #fff;
}
.faq_left_wrap{
	padding-top: 20px;
}

.faq_left_wrap.loading{
	background-position: center;
}

.faq_li_t{
	font-size: 18px;
	color: #29485f;
	height: 38px;
	line-height: 38px;
	padding-left: 24px;
	border-left: 4px solid #1eb7c5;
}

.faq_li_t.shouqi{
	background-image: url('img/shouqi.png');
	margin-bottom: 1px;
}
.faul_parent {
	overflow: hidden;
}
.faq_ul{
	overflow: hidden;
	
}
.faq_li_list ul{
	padding: 5px 0;
}
.faq_li_list ul li{
	text-align: left;
	padding-left: 45px;
	height: 40px;
	line-height: 40px;
	font-size: 14px;
	color: #5e839e;
	cursor: pointer;
}

.faq_li_list ul li.active, .faq_li_list ul li:hover{
	background: #1ab4c2;
	color: #fff;
}

.faq_center_content{
	padding: 20px;
}